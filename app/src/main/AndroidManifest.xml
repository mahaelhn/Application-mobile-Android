<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    package="com.example.myapplication"
    android:name="myapplication.App"
    tools:overrideLibrary="myapplication.App"
    app:showAsAction="never">
    <uses-permission android:name="android.permission.WAKE_LOCK" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <uses-library
       android:name="com.google.android.maps"
            android:required="true" />

        <!--
               Set to true if your app is Standalone, that is, it does not require the handheld
               app to run.
        -->
        <activity android:name=".NewTechnician"></activity>
        <activity android:name=".NewIntervention"></activity>
        <activity android:name=".ViewAccounts"></activity>
        <activity android:name=".ViewIntervention"></activity>
        <activity android:name=".Register"></activity>
        <activity android:name=".MenuTech"></activity>
        <activity android:name=".Menu"></activity>
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>