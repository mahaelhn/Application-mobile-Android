<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ViewIntervention"
    >

    <ImageView
        android:layout_width="366dp"
        android:layout_height="33dp"
        android:layout_marginBottom="92dp"
        android:src="@drawable/category"
        app:layout_constraintBottom_toTopOf="@+id/textView2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/menu"
        android:layout_width="115dp"
        android:layout_height="36dp"
        android:layout_gravity="center_horizontal"
        android:text="Activities"
        android:textSize="25dp"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="146dp"
        tools:layout_editor_absoluteY="75dp" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Technicians"
        tools:layout_editor_absoluteX="22dp"
        tools:layout_editor_absoluteY="141dp"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Interventions"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="146dp"
        tools:layout_editor_absoluteY="141dp" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Categories"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="292dp"
        tools:layout_editor_absoluteY="141dp" />

    <ListView
        android:id="@+id/lvTechnicians"
        android:layout_width="99dp"
        android:layout_height="231dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="45dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView" />

    <ListView
        android:id="@+id/lvInterventions"
        android:layout_width="112dp"
        android:layout_height="230dp"
        android:layout_marginStart="19dp"
        android:layout_marginTop="46dp"
        app:layout_constraintStart_toEndOf="@+id/lvTechnicians"
        app:layout_constraintTop_toBottomOf="@+id/textView3"
        />
    <ListView
        android:id="@+id/lvCategories"
        android:layout_width="107dp"
        android:layout_height="229dp"
        android:layout_marginStart="28dp"
        android:layout_marginTop="46dp"
        app:layout_constraintStart_toEndOf="@+id/lvInterventions"
        app:layout_constraintTop_toBottomOf="@+id/textView3" />

    <TextView
        android:id="@+id/tvTech"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="none"
        tools:layout_editor_absoluteX="31dp"
        tools:layout_editor_absoluteY="482dp"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/tvInter"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="none"
        tools:layout_editor_absoluteX="167dp"
        tools:layout_editor_absoluteY="478dp"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/tvCateg"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="none"
        tools:layout_editor_absoluteX="302dp"
        tools:layout_editor_absoluteY="482dp"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/buttonLink"
        android:layout_width="334dp"
        android:layout_height="53dp"
        android:text="Add"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="33dp"
        tools:layout_editor_absoluteY="572dp" />

    <Button
        android:id="@+id/buttonReturn"
        android:layout_width="330dp"
        android:layout_height="53dp"
        android:text="Return"
        tools:layout_editor_absoluteX="35dp"
        tools:layout_editor_absoluteY="643dp"
        tools:ignore="MissingConstraints" />


</androidx.constraintlayout.widget.ConstraintLayout>